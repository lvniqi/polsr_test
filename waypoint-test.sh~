#!/bin/sh
#"0=POLSR;1=OLSR;2=AODV;3=DSDV;"
#PROTOCOL="0 1 2 3"
PROTOCOL="0 1 2 3"
SPEED="0 5 10 15 20 25 30 35 40"
SEED="1"
echo UAVNET TEST

echo TEST Throughput of Different Protocol In Different Speed
for protocol in $PROTOCOL
do
  for speed in $SPEED
  do
  	for seed in $SEED
  	do
			echo protocol $protocol, speed $speed, seed $seed
			../../waf --run "waypoint-test --protocol=$protocol --speed=$speed --seed=$seed --test="
			../../waf --run "waypoint-test --protocol=$protocol --speed=$speed --seed=$seed --test=1"
		done
  done
done

